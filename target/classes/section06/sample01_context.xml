<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- 
		타입과 일치하는 빈이 없을 경우 : 예외발생없음, null 처리  
		타입과 일치하는 빈이 2개 이상 경우 : 예외발생, primary속성으로 우선순위 부여
	--> 
	<bean id="demoDao2" class="section06.sample01.DemoDao"/>
		
	<bean id="demoDao" class="section06.sample01.DemoDao" primary="true"/>
	 
	<bean id="demoDetails" class="section06.sample01.DemoDetails" scope="prototype"/>
	<bean id="demoService" class="section06.sample01.DemoService" 
		scope="prototype" autowire="byType"/>

</beans>


